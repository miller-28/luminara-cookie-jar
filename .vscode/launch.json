{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Luminara Cookies Plugin - Run CLI Tests",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "test"
            ],
            "cwd": "${workspaceFolder}/test-cli",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Luminara Cookies Plugin - Run Linting",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "lint"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Luminara Cookies Plugin - Lint and Fix",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "lint:fix"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}